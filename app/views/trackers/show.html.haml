%div#tracker.info
  %p= link_to @tracker.name, @tracker.url
  %div.credentials
    %p
      %span Login:
      = @tracker.login
    %p
      %span Password:
      = @tracker.password
  %div
    %p Categories:
    %ul
      - @tracker.media_categories.each do |category|
        %li= category.name
= link_back_to :trackers
= link_to_edit @tracker
